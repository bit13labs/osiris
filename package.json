{
  "name": "osiris",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "$(npm bin)/babel-node ./bin/www",
    "exec": "nodemon ./bin/www --exec babel-node",
    "test": "jshint . --verbose && eslint . && JASMINE_CONFIG_PATH=tests/unit/.jasmine.json $(npm bin)/babel-node $(npm bin)/babel-istanbul cover $(npm bin)/jasmine",
    "cover": "JASMINE_CONFIG_PATH=./tests/unit/.jasmine.json $(npm bin)/babel-node $(npm bin)/babel-istanbul cover $(npm bin)/jasmine && $(npm bin)/codecov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bit13labs/osiris.git"
  },
  "bugs": {
    "url": "https://github.com/bit13labs/osiris/issues"
  },
  "author": "Bit13 Labs",
  "license": "Apache-2.0",
  "babel": {
    "presets": [
      "latest"
    ],
    "plugins": [
      "transform-runtime",
      "transform-decorators-legacy",
      "transform-class-properties"
    ]
  },
  "dependencies": {
    "async": "^2.4.1",
    "babel-cli": "^6.24.1",
    "babel-istanbul": "^0.12.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "body-parser": "^1.17.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-session": "^1.15.3",
    "hbs": "^4.0.1",
    "jasmine": "^2.6.0",
    "lodash": "^4.17.4",
    "merge": "^1.2.0",
    "morgan": "^1.8.2",
    "npm": "^5.0.3",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "promise": "^7.1.1",
    "request": "^2.81.0",
    "semver": "^5.3.0",
    "serialport": "^4.0.7",
    "serve-favicon": "^2.4.3",
    "uuid": "^3.0.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "chai": "^4.0.2",
    "codecov": "^2.2.0",
    "eslint": "^3.19.0",
    "istanbul": "^0.4.5",
    "jasmine-node": "^1.14.5",
    "jshint": "^2.9.4",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0"
  }
}
